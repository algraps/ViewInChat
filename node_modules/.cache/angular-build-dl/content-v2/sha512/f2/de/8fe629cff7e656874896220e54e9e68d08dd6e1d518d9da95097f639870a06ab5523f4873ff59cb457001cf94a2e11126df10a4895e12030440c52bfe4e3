false;\r\n      }\r\n    }\r\n  }\r\n\r\n  private isAttachable(track: RemoteTrack): track is RemoteAudioTrack | RemoteVideoTrack {\r\n    return !!track &&\r\n      ((track as RemoteAudioTrack).attach !== undefined ||\r\n        (track as RemoteVideoTrack).attach !== undefined);\r\n  }\r\n\r\n  private isDetachable(track: RemoteTrack): track is RemoteAudioTrack | RemoteVideoTrack {\r\n    return !!track &&\r\n      ((track as RemoteAudioTrack).detach !== undefined ||\r\n        (track as RemoteVideoTrack).detach !== undefined);\r\n  }\r\n\r\n  //#endregion\r\n\r\n}\r\n","export default \".participantotm {\\n  height: 130px;\\n  background-color: #000000;\\n  border-radius: 10px;\\n  margin-bottom: 10px;\\n  position: relative;\\n  border: 1px solid #423a3a;\\n}\\n/*# sourceMappingURL=data:application/json;base64, */\"","import { Component, OnInit } from '@angular/core';\r\n\r\n@Component({\r\n  selector: 'app-otmvideo',\r\n  templateUrl: './otmvideo.component.html',\r\n  styleUrls: ['./otmvideo.component.scss']\r\n})\r\nexport class OtmvideoComponent implements OnInit {\r\n\r\n  constructor() { }\r\n\r\n  ngOnInit(): void {\r\n  }\r\n\r\n}\r\n","export default \".popover-content {\\n  overflow-y: scroll;\\n  max-height: 60%;\\n}\\n\\n.videosection {\\n  height: 55vh;\\n}\\n\\n.chatsection {\\n  height: 55vh;\\n}\\n\\n.notessection {\\n  height: 29vh !important;\\n  max-height: 29vh !important;\\n  overflow-y: auto !important;\\n}\\n\\n.chatshistory {\\n  height: 30vh;\\n  overflow-y: auto;\\n}\\n\\n/*css form chat grid*/\\n\\n.chat-segment-sent .chat-message {\\n  background: #351E58;\\n  border-radius: 10px 10px 0px 10px;\\n  color: #fff;\\n  text-align: left;\\n  margin-bottom: 10px;\\n}\\n\\n.other-chat-message {\\n  position: relative;\\n  left: 3.5rem;\\n}\\n\\n.chat-message {\\n  padding: 0.75rem 1rem;\\n  border-radius: 0.625rem;\\n  position: relative;\\n  display: inline-block;\\n  width: calc(100% - 35px);\\n  margin-left: 5px;\\n}\\n\\n.chat-segment-sent