            this.subscription.unsubscribe();
        }
        catch (ex) {
            console.log(ex.message);
        }
    }
    // jobToRunBeforeMinutesOfMeetingEnding() {
    //   var _that = this;
    //   var schedule = require('node-schedule');
    //   var j = schedule.scheduleJob(this.meetingEndDateTime.add(-5, 'minutes')._d, function () {
